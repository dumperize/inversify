{
  "name": "journal_app",
  "version": "0.1.0",
  "description": "Web версия приложения Электронный Журнал на React+MobX",
  "keywords": [
    "react",
    "mobx",
    "education"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.cosysoft.ru/education-apps/journal_app.git"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "set:git:hooks": "node config/setGitHooks.js",
    "generate:style:typings": "node config/generateStyleTypings.js",
    "clear:style:typings": "node config/clearStyleTypings.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config config/webpack/webpack.dev.config.js",
    "build:dev:check:styles": "npm run generate:style:typings && npm run build:dev && npm run clear:style:typings",
    "build:prod": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.config.js",
    "serve": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack/webpack.dev.config.js",
    "start": "npm run set:git:hooks && npm run clear:style:typings && npm run serve",
    "pretest": "jest --clearCache",
    "lint": "eslint . --ext .js,.ts,.tsx --cache",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix --cache",
    "test": "cross-env platform=web && jest",
    "test:coverage": "jest --coverage",
    "stylelint": "stylelint app/**/*.{scss,sass,css}",
    "stylelint:fix": "stylelint app/**/*.{scss,sass,css} --fix",
    "storybook": "start-storybook -p 9009 -c config/storybook"
  },
